{
  "manifest_version": 3,
  "name": "Tab Garbage Collector",
  "version": "0.0.1",
  "description": "Tab GC",
  "permissions": [
    "tabs",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  }
}